# Constants properties

# backend_start_time
# used to mark the unix time stamp (in milli-seconds) when the backend was
#   first started in production
# default: 1483372235000 (2 Jan 2017)
# current: 1484784000000 (19 Jan 2017)
backend_start_time=1484784000000

# default_region
# usually this value shouldn't be altered once this service starts running in
#   production
# default: Lala_Land
default_region=Lala_Land

# world_region
# this region represents the whole world
# usually this value shouldn't be altered once this service starts running in
#   production
# world_region: Planet_Earth
world_region=Planet_Earth

# geo_grid_dimension_metres
# usually this value shouldn't be altered once this service starts running in
#   production
# default: 10000
geo_grid_dimension_metres=10000

# default_grid_id
# usually this value shouldn't be altered once this service starts running in
#   production
# default: -1
default_grid_id=-1
